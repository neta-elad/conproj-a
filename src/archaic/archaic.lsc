#include "../base/base.lsc"

ancient-derivations:
    unchanged

resonant-codas ltr:
    [vowel *stress] => * / [vowel] @resonant _ 

syllables:
    [consonant] [vowel] @resonant?

nominalizers:
    unchanged

regressive-uvular-spreading:
    [velar *labialized] => [uvular] / _ (![liquid])* [uvular]

romanizer-archaic-phonetic:
    unchanged

romanizer-archaic:
#include "../base/romanizer.lsc"
then:
    χ => qh
    ɴ => nq
then:
    [+labialized] * => [*labialized] w